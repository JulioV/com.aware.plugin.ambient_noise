<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aware.plugin.ambient_noise"
    android:versionCode="4"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">

        <service android:name="com.aware.plugin.ambient_noise.Plugin" android:exported="true"></service>
        <activity android:name="com.aware.plugin.ambient_noise.Settings" android:exported="true" android:configChanges="keyboardHidden|orientation|screenSize"></activity>
        <provider android:name="com.aware.plugin.ambient_noise.Provider"
            android:authorities="com.aware.plugin.ambient_noise.provider.ambient_noise" android:writePermission="com.aware.WRITE_CONTEXT_DATA" android:exported="true" android:readPermission="com.aware.READ_CONTEXT_DATA"></provider>
    </application>

</manifest>
